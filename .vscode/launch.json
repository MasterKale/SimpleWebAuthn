  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Launch Example Server",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/**/node_modules/**",
      ],
      "cwd": "${workspaceFolder}/example",
    }
  ]
}
