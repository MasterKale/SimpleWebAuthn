    "@peculiar/asn1-schema": "^2.1.7",
    "@peculiar/asn1-x509": "^2.1.7",
    "base64url": "^3.0.1",
    "cbor": "^5.1.0",
    "debug": "^4.3.2",
    "jsrsasign": "^10.4.0",
    "jwk-to-pem": "^2.0.4",
    "node-fetch": "^2.6.0"
  },
  "gitHead": "ba039fdd5fdff87f78d3bd246e9bea5f7aa39ccb",
  "devDependencies": {
    "@simplewebauthn/typescript-types": "*",
    "@types/cbor": "^5.0.1",
    "@types/debug": "^4.1.7",
    "@types/jsrsasign": "^8.0.13",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/node-fetch": "^2.5.12"
  }
